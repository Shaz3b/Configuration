set rtp+=/Users/Shazeb/Library/Python/3.7/lib/python/site-packages/powerline/bindings/vim/
set laststatus=2
set t_Co=256
syntax on
set tabstop=2
set shiftwidth=2
set expandtab
set ai
set number
set hlsearch
set ruler
noremap <Up> <Nop>
noremap <Down> <Nop>
noremap <Left> <Nop>
noremap <Right> <Nop>
set incsearch
highlight Comment ctermfg=green
set clipboard=unnamed
set backspace=indent,eol,start
